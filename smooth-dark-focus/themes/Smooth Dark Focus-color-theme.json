{
  "name": "Smooth Dark Focus",
  "type": "dark",
  "semanticHighlighting": true,
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91969D60"
			}
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted",
                "string.other"
            ],
            "settings": {
                "foreground": "#e4bd68"
            }
        },
        {
            "name": "String Features",
            "scope": [
              "string.embedded.begin",
              "string.embedded.end",
              "punctuation.definition.template-expression",
              "punctuation.section.embedded",
              "punctuation.definition.string",
              "punctuation.definition.interpolation",
            ],
            "settings": {
              "foreground": "#3b8ba5"
            }
        },
        {
            "name": "Default Embedded Color",
            "scope": [
              "punctuation.section.embedded.begin.js",
              "punctuation.section.embedded.end.js",
              "punctuation.section.embedded.begin.erb",
              "punctuation.section.embedded.end.erb",
              "source.elixir.embedded",
              "punctuation.separator",
              "punctuation.accessor"
            ],
            "settings": {
              "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "meta.property-list",
                "meta.block",
                "punctuation.terminator",
                "punctuation.section.block",
                "meta.structure.dictionary"
            ],
            "settings": {
                "foreground": "#75cbdf"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
              "foreground": "#b06fd0"
            }
        },
        {
            "name": "Language Constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#75cbdf"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
              "constant.character",
              "constant.other"
            ],
            "settings": {
              "foreground": "#5179ce"
            }
        },
        {
            "name": "Language Variable",
            "scope": "variable.language",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Keyword",
            "scope": [
              "keyword",
              "keyword.operator.logical",
              "keyword.operator.constructor"
            ],
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Keyword Operator",
            "scope": "keyword.operator",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Types",
            "scope": "storage.type",
            "settings": {
              "foreground": "#3b8ba5"
            }
        },
        {
            "name": "Class",
            "scope": [
              "entity.name.class",
              "entity.name.module",
              "entity.name.type",
              "storage.identifier",
              "support.class"
            ],
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Object Variable",
            "scope": [
              "variable.other.object",
              "variable.other.constant",
              "variable.other.global",
              "variable.other.readwrite.class",
              "variable.other.readwrite.instance",
              "variable.other.readwrite.batchfile",
              "variable.readwrite",
              "variable.readwrite.other.block"
            ],
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Other Variable",
            "scope": [
              "variable.other",
              "variable.other.property",
              "variable.other.block"
            ],
            "settings": {
              "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Inherited Class",
            "scope": "entity.other.inherited-class",
            "settings": {
              "foreground": "#67a3e9"
            }
        },
        {
            "name": "Package Import",
            "scope": [
              "storage.modifier.import",
              "storage.modifier.package"
            ],
            "settings": {
              "foreground": "#e4bd68"
            }
        },
        {
            "name": "Function name",
            "scope": [
              "entity.name.function",
              "support.function"
            ],
            "settings": {
              "foreground": "#a6d66e"
            }
        },
        {
            "name": "Function argument",
            "scope": [
              "variable.parameter",
              "entity.name.variable.parameter",
              "parameter.variable"
            ],
            "settings": {
              "fontStyle": "italic",
              "foreground": "#c27c43"
            }
        },
        {
            "name": "Function call",
            "scope": "entity.name.function-call",
            "settings": {
              "foreground": "#ff00ff"
            }
        },
        {
            "name": "Builtin Functions",
            "scope": [
              "function.support.builtin",
              "function.support.core"
            ],
            "settings": {
              "foreground": "#00ffa0"
            }
        },
        {
            "name": "Tag Name",
            "scope": [
              "entity.name.tag",
              "entity.name.tag.class.js"
            ],
            "settings": {
              "foreground": "#EE4A66",
              "fontStyle": ""
            }
        },
        {
            "name": "Tag Class and ID",
            "scope": [
              "entity.name.tag.class",
              "entity.name.tag.id"
            ],
            "settings": {
              "foreground": "#e4bd68"
            }
        },
        {
            "name": "HTML Tag Attribute",
            "scope": "meta.attribute",
            "settings": {
              "foreground": "#a6d66e"
            }
        },
        {
            "name": "Library constant",
            "scope": "support.constant",
            "settings": {
              "foreground": "#c27c43"
            }
        },
        {
            "name": "Library class/type",
            "scope": [
              "support.type",
              "support.variable"
            ],
            "settings": {
              "foreground": "#3b8ba5"
            }
        },
        {
            "name": "Json Property",
            "scope": "support.dictionary.json",
            "settings": {
              "foreground": "#3b8ba5"
            }
        },
        {
            "name": "StyleSheet Property name",
            "scope": [
              "support.type.property-name.css",
              "support.type.property-name.scss",
              "support.type.property-name.less",
              "support.type.property-name.sass"
            ],
            "settings": {
              "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Stylesheet Selector",
            "scope": "meta.selector",
            "settings": {
                "foreground": "#e4bd68",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS: Pseudo Attribute Names",
            "scope": [
              "entity.other.attribute-name.pseudo-class.css",
              "entity.other.attribute-name.pseudo-class.scss",
              "entity.other.attribute-name.pseudo-class.less",
              "entity.other.attribute-name.pseudo-class.sass",
              "entity.other.attribute-name.pseudo-element.css",
              "entity.other.attribute-name.pseudo-element.scss",
              "entity.other.attribute-name.pseudo-element.less",
              "entity.other.attribute-name.pseudo-element.sass"
            ],
            "settings": {
              "foreground": "#75cbdf"
            }
        },
        {
            "name": "StyleSheet Property value",
            "scope": [
              "support.constant.css",
              "support.constant.scss",
              "support.constant.less",
              "support.constant.sass"
            ],
            "settings": {
              "foreground": "#ffa000"
            }
        },
        {
            "name": "StyleSheet Variable",
            "scope": [
              "variable.css",
              "variable.scss",
              "variable.less",
              "variable.sass"
            ],
            "settings": {
              "foreground": "#3b8ba5"
            }
        },
        {
            "name": "StyleSheet Variable String",
            "scope": [
              "variable.css.string",
              "variable.scss.string",
              "variable.less.string",
              "variable.sass.string"
            ],
            "settings": {
              "foreground": "#e4bd68"
            }
        },
        {
            "name": "StyleSheet Unit",
            "scope": [
              "unit.css",
              "unit.scss",
              "unit.less",
              "unit.sass"
            ],
            "settings": {
              "foreground": "#ff00a0"
            }
        },
        {
            "name": "StyleSheet Function",
            "scope": [
              "function.css",
              "function.scss",
              "function.less",
              "function.sass"
            ],
            "settings": {
              "foreground": "#ffa000"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "Invalid deprecated",
            "scope": "invalid.deprecated",
            "settings": {
              "foreground": "#B1405F"
            }
        },
        {
            "name": "JSON String",
            "scope": "support.type.property-name.json",
            "settings": {
              "foreground": "#B1405F"
            }
        },
        {
            "name": "Link",
            "scope": "string.detected-link",
            "settings": {
              "foreground": "#75cbdf"
            }
        },
        {
            "name": "diff.header",
            "scope": [
              "meta.diff",
              "meta.diff.header"
            ],
            "settings": {
              "foreground": "#00ffa0"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
              "foreground": "#EE4A66"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
              "foreground": "#a6d66e"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
              "foreground": "#75cbdf"
            }
        },
	],
